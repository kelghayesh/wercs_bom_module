@model IEnumerable<SDS.SDSRequest.Models.FormulaImportRequestMessage>

@{
    ViewBag.Title = "Request Queue Item Messages";
}

<h2>Request Queue Item Messages</h2>


<p>
    @if (Model.Count() > 0) { 
    @Html.ActionLink("Back to Request Queue", "RequestQueueIndex", "FormulaImportRequest", new { id = Model.First().RequestId, sourceSystem = Model.First().SourceSystem }, null)
    }
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Requested_Part)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Processed_PartKey)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MessageType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MessageText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedDatestamp)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Requested_Part)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Processed_PartKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MessageType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MessageText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedDatestamp)
        </td>
        <!--
        <td>
            @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
        </td>
        -->
    </tr>
}

</table>
